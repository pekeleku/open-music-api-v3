{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle":"off",
        "no-unused-expressions":"off",
        "no-undef":"off",
        "no-console": "off",
        "linebreak-style": "off",
        "camelcase":"off",
        "class-methods-use-this":"off",
        "no-shadow":"off"
        
    }
    
}
